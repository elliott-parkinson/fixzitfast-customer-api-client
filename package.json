{
  "name": "fixzitfast-customer-api-client",
  "version": "1.0.0",
  "description": "Fixzitfast Customer Api Client",
  "main": "src/api/index.js",
  "alias": {
    "fixzitfast-api": "./modules/api-client/rest/src"
  },
  "scripts": {
    "watch:browser:testbed": "npx parcel src/modules/fixzitfast/api-client-web-test/build/www/index.html --out-dir bin/www/testbed --port 2221",
    "build:browser:testbed": "npx parcel build src/modules/fixzitfast/api-client-web-test/build/www/index.html --out-dir bin/www/testbed",
    "watch:browser:portal": "npx parcel src/modules/fixzitfast/api-client-web-portal/build/www/index.html --out-dir bin/www/portal --port 2223",
    "build:browser:portal": "npx parcel build src/modules/fixzitfast/api-client-web-portal/build/www/index.html --out-dir bin/www/portal && cp src/modules/fixzitfast/api-client-web-portal/build/www/_redirects bin/www/portal",
    "watch:app:portal": "npx parcel src/modules/fixzitfast/customer-mobile-app/build/www/index.html --out-dir bin/www/portal --port 2222",
    "build:app:portal": "npx parcel build src/modules/fixzitfast/customer-mobile-app/build/www/index.html --out-dir bin/www/portal",
    "test": "mocha -r ts-node/register ./src/modules/fixzitfast/api-client/test/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^7.0.1",
    "animate.css": "^4.1.1",
    "axios": "^0.19.2",
    "cleave.js": "^1.6.0",
    "colors": "^1.3.3",
    "es6-shim": "^0.35.6",
    "moment": "^2.29.1",
    "node": "^13.6.0",
    "pretty-rating-react": "^1.1.0",
    "react": "^16.14.0",
    "react-booking-selector": "^1.0.2",
    "react-bootstrap-typeahead": "^5.1.2",
    "react-rating": "^2.0.5",
    "react-stepper-horizontal": "^1.0.11",
    "reflect-metadata": "^0.1.13",
    "serializer.ts": "0.0.12",
    "src": "^1.1.2",
    "styled-components": "^5.2.0",
    "ts-node": "^9.0.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@types/node": "^12.19.0",
    "@types/react": "^16.9.53",
    "@types/reactstrap": "^8.2.0",
    "@types/reflect-metadata": "^0.1.0",
    "chai": "^4.2.0",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "mocha": "^7.2.0",
    "mocha-steps": "^1.3.0",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0",
    "sass": "^1.27.0",
    "typescript": "^3.9.7"
  }
}
